{
  "name": "koishi-plugin-comfyui-workflow",
  "description": "一个Koishi适用的ComfyUI客户端插件",
  "version": "0.0.3",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist",
    "workflows"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npx tsc && npx esbuild src/index.tsx --bundle --platform=node --target=node20 --jsx=transform --jsx-factory=h --jsx-import-source=@satorijs/element --outfile=lib/index.js",
    "build:tsc": "npx tsc",
    "build:bundle": "npx esbuild src/index.tsx --bundle --platform=node --target=node20 --jsx=transform --jsx-factory=h --jsx-import-source=@satorijs/element --outfile=lib/index.js"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "comfyui"
  ],
  "contributors": [
    "WuRunBear"
  ],
  "homepage": "https://github.com/WuRunBear/koishi-plugin-comfyui-client",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WuRunBear/koishi-plugin-comfyui-client.git"
  },
  "peerDependencies": {
    "koishi": "^4.18.0"
  },
  "dependencies": {
    "@satorijs/element": "^3.1.8",
    "form-data": "^4.0.4",
    "openai": "^5.13.1",
    "uuid": "^11.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "esbuild": "^0.27.0",
    "yml-register": "^1.2.5"
  }
}
